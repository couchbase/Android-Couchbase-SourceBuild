# Important paths for Android binaries

export COUCH_ANDROID_HOME=$(pwd)
export ANDROID_SDK_ROOT=$HOME/lib/android
export ANDROID_APP_NAME="com.test.couch"
export ANDROID_TARGET=froyo

paths="
$(pwd)/scripts
$ANDROID_SDK_ROOT/other/apk
$ANDROID_SDK_ROOT/$ANDROID_TARGET/prebuilt/linux-x86/toolchain/arm-eabi-4.4.0/bin/
"

for i in $paths
do
    if [[ -d $i ]]; then
        PATH=$PATH:$i
    fi
done

export PATH
